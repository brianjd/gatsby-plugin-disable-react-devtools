{
  "name": "gatsby-plugin-disable-react-devtools",
  "description": "A plugin to disable React Dev Tools",
  "version": "0.0.0-development",
  "author": "Brian Doyle <brian@tf83.com>",
  "bugs": {
    "url": "https://github.com/brianjd/gatsby-plugin-disable-react-devtools/issues"
  },
  "main": "dist/index.js",
  "repository": "https://github.com/brianjd/gatsby-plugin-disable-react-devtools",
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist --ignore *.test.js",
    "commit": "git-cz",
    "format": "prettier --write 'src/**/*.js' 'README.md'",
    "prepublish": "npm run build",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "watch": "npm run build -- --watch",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "peerDependencies": {
    "gatsby": "^2.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "commitizen": "^3.0.0",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "prettier": "^1.8.2",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "travis-deploy-once": "^5.0.11"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "react-devtools"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
